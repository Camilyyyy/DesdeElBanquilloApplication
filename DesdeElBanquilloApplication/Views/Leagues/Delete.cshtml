@model DesdeElBanquilloApplication.ViewModels.LeagueViewModel
<h1>Eliminar Liga</h1>
<h3>¿Seguro que quieres eliminar esta liga?</h3>
<dl class="row">
    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Name)</dt>
    <dd class="col-sm-10">@Html.DisplayFor(m => m.Name)</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="IdLeague" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>